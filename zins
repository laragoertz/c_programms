#include <stdio.h>

int main()
{
    double Zins;
    double AB;
    double AD;
    double f;
    printf("\nGeben Sie den Zins ein\n");
    scanf("%lf", &Zins);
    printf("\nGeben Sie den Anlagebetrag ein\n");
    scanf("%lf", &AB);
    printf("\nGeben Sie die Anlagedauer ein\n");
    scanf("%lf", &AD);
    f=rechnen(Zins,AB,AD);
    
}
  
      
int rechnen(double Zins, double AB, double AD){
    printf("das ist das unterprogramm");
    double c=0.0;
do{
    AB=AB*(1+Zins);
}
     while (c<AD);
    printf("Der Anlagebetrag ist %lf\n",AB);
    return c;
}
  
  
